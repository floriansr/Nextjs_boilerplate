{
  "name": "boilerplate_nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "all": "yarn && yarn build && yarn format && yarn lint && yarn ts:check && yarn coverage && yarn test && yarn test:ci && yarn dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx,.test.tsx .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,scss,json,babelrc,test.tsx,yml}' --config ./.prettierrc",
    "test": "jest --runInBand",
    "test:ci": "yarn test --ci",
    "test:watch": "yarn test --watch",
    "ts:coverage": "yarn type-coverage",
    "ts:check": "tsc --noEmit",
    "coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "babel-jest": "^26.6.3",
    "cypress": "^6.0.0",
    "framer-motion": "^2.9.4",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "next": "latest",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "latest",
    "styled-system": "^5.1.5",
    "ts-jest": "^26.4.4",
    "type-coverage": "^2.14.5",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@styled-system/should-forward-prop": "^5.1.5",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "@types/styled-system": "^5.1.10",
    "@types/styled-system__css": "^5.0.14",
    "@types/styled-system__should-forward-prop": "^5.1.2",
    "@types/webpack": "^4.41.25",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-images": "^1.6.2",
    "nprogress": "^0.2.0",
    "prettier": "^2.2.0"
